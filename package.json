{
  "name": "observable-duck",
  "version": "0.3.5",
  "description": "pure logic DuckComponent by redux & rxjs",
  "keywords": [
    "redux",
    "rxjs",
    "observable",
    "duck",
    "observable-duck"
  ],
  "files": [
    "build",
    "packages",
    "typings",
    "tsconfig.json",
    "tsconfig.build.json"
  ],
  "types": "./typings/index.d.ts",
  "typings": "./typings/index.d.ts",
  "main": "./build/core.js",
  "module": "./build/core.mjs",
  "exports": {
    ".": {
      "import": "./build/core/index.mjs",
      "require": "./build/core/index.js",
      "default": "./build/core/index.mjs"
    },
    "./operator": {
      "import": "./build/operator/index.mjs",
      "require": "./build/operator/index.js",
      "default": "./build/operator/index.mjs"
    },
    "./decorator": {
      "import": "./build/decorator/index.mjs",
      "require": "./build/decorator/index.js",
      "default": "./build/decorator/index.mjs"
    },
    "./helper": {
      "import": "./build/helper/index.mjs",
      "require": "./build/helper/index.js",
      "default": "./build/helper/index.mjs"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ienyh/observable"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "tsc": "rm -rf typings && tsc --declaration --emitDeclarationOnly --project tsconfig.build.json",
    "build": "vite build --mode production",
    "prebuild": "npm run tsc"
  },
  "dependencies": {
    "redux": "^5.0.1",
    "redux-observable-action": "^0.0.7"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-redux": "^9.0.1",
    "reflect-metadata": ">=0.2.1",
    "rxjs": ">=7.8.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.6.1",
    "@types/react": "^18.3.8",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "reselect": "^5.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vitest": "^2.1.1"
  },
  "packageManager": "pnpm@9.10.0"
}
